{
  "name": "primefaces",
  "version": "1.0.0",
  "description": "TypeScript definitions for the client side API of PrimeFaces",
  "main": "src/index.js",
  "license": "MIT",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git://github.com/primefaces/primefaces.git",
    "directory": "src/main/type-definitions"
  },
  "types": "./PrimeFaces.d.ts",
  "typeScriptVersion": "3.2",
  "scripts": {
    "generate-d-ts": "node src/index.js",
    "generate-typedoc": "node node_modules/typedoc/bin/typedoc",
    "postinstall": "patch-package",
    "npm-publish": "node src/npm-publish.js",
    "test-aggregate": "node test/index.js -- AggregateTest",
    "test-transform": "node test/index.js -- TransformTest",
    "test-tsprocess": "node test/index.js -- TsProcessTest",
    "test-tsvalidate": "node test/index.js -- TsValidateTest",
    "test": "node test/index.js -- AggregateTest TransformTest TsProcessTest TsValidateTest",
    "validate": "node node_modules/typescript/bin/tsc --project tsconfig.json"
  },
  "jsdocs": {
    "exclusionTags": [
      "ignore",
      "exclude",
      "hidden"
    ],
    "inputDir": "../resources/META-INF/resources/primefaces",
    "declarationOutputDir": "../../../docs",
    "outputFilename": "PrimeFaces.d.ts",
    "moduleOutputFilename": "PrimeFaces-module.d.ts",
    "typedocOutputDir": "../../../docs/jsdocs",
    "severitySettings": {},
    "skipEsLint": false,
    "skipPostProcess": false,
    "skipTypedocGenerate": false,
    "verbose": false
  },
