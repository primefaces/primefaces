<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "../misc/component.dtd" [
<!ENTITY standard_uicomponent_attributes		SYSTEM "../entities/standard_uicomponent_attributes.xml">
]>
<component>
    <tag>treeNode</tag>
    <tagClass>org.primefaces.component.tree.UITreeNodeTag</tagClass>
    <componentClass>org.primefaces.component.tree.UITreeNode</componentClass>
    <componentType>org.primefaces.component.UITreeNode</componentType>
    <componentFamily>org.primefaces.component</componentFamily>
    <parent>javax.faces.component.UIColumn</parent>
    <description>TreeNode is used with Tree component to represent a node in tree.</description>
    <attributes>
        &standard_uicomponent_attributes;
        <attribute>
            <name>type</name>
            <required>false</required>
            <type>java.lang.String</type>
            <defaultValue>default</defaultValue>
            <description>Type of the tree node. Default is "default"</description>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <required>false</required>
            <type>java.lang.String</type>
            <description>Style class to apply a particular tree node type.</description>
        </attribute>
        <attribute>
            <name>icon</name>
            <required>false</required>
            <type>java.lang.String</type>
            <description></description>
        </attribute>
        <attribute>
            <name>expandedIcon</name>
            <required>false</required>
            <description></description>
            <type>java.lang.String</type>
        </attribute>
        <attribute>
            <name>collapsedIcon</name>
            <required>false</required>
            <type>java.lang.String</type>
            <description></description>
        </attribute>
        <attribute>
            <name>ariaLabel</name>
            <required>false</required>
            <type>java.lang.String</type>
            <description></description>
        </attribute>
    </attributes>
</component>
