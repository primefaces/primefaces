<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "../misc/component.dtd" [
<!ENTITY standard_uicomponent_attributes		SYSTEM "../entities/standard_uicomponent_attributes.xml">
]>
<component>
	<tag>cache</tag>
	<tagClass>org.primefaces.component.cache.UICacheTag</tagClass>
	<componentClass>org.primefaces.component.cache.UICache</componentClass>
	<componentType>org.primefaces.component.Cache</componentType>
	<componentFamily>org.primefaces.component</componentFamily>
	<rendererType>org.primefaces.component.UICacheRenderer</rendererType>
	<rendererClass>org.primefaces.component.cache.UICacheRenderer</rendererClass>
	<parent>javax.faces.component.UIPanel</parent>
    <description>Cache component is used to reduce page load time by caching the content after initial rendering.</description>
    <attributes>
		&standard_uicomponent_attributes;
        <attribute>
			<name>disabled</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
            <defaultValue>false</defaultValue>
            <description>Disables caching.</description>
		</attribute>
        <attribute>
			<name>region</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Unique id of the cache region, defaults to view id.</description>
		</attribute>
		<attribute>
			<name>key</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Unique id of the cache entry in region, defaults to client id of component.</description>
		</attribute>
        <attribute>
			<name>processEvents</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
            <defaultValue>false</defaultValue>
            <description>When enabled, lifecycle events such as button actions are executed. Default is false.</description>
		</attribute>
	</attributes>
</component>