<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "../misc/component.dtd" [
<!ENTITY standard_uicomponent_attributes		SYSTEM "../entities/standard_uicomponent_attributes.xml">
<!ENTITY widget_var_attribute					SYSTEM "../entities/widget_var_attribute.xml">
]>
<component>
    <tag>bubbleChart</tag>
    <tagClass>org.primefaces.component.chart.bubble.BubbleChartTag</tagClass>
    <componentClass>org.primefaces.component.chart.bubble.BubbleChart</componentClass>
    <componentType>org.primefaces.component.chart.BubbleChart</componentType>
    <componentFamily>org.primefaces.component</componentFamily>
    <rendererType>org.primefaces.component.chart.BubbleChartRenderer</rendererType>
    <rendererClass>org.primefaces.component.chart.bubble.BubbleChartRenderer</rendererClass>
    <parent>org.primefaces.component.chart.UIChart</parent>
    <interfaces>
        <interface>
            <name>org.primefaces.component.api.Widget</name>
        </interface>
    </interfaces>
    <attributes>
		&standard_uicomponent_attributes;
		&widget_var_attribute;
        <attribute>
            <name>value</name>
            <required>false</required>
            <type>org.primefaces.model.chart.BubbleChartModel</type>
        </attribute>
        <attribute>
            <name>style</name>
            <required>false</required>
            <type>java.lang.String</type>
            <ignoreInComponent>true</ignoreInComponent>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <required>false</required>
            <type>java.lang.String</type>
            <ignoreInComponent>true</ignoreInComponent>
        </attribute>
        <attribute>
            <name>title</name>
            <required>false</required>
            <type>java.lang.String</type>
            <ignoreInComponent>true</ignoreInComponent>
        </attribute>
        <attribute>
            <name>shadow</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
            <ignoreInComponent>true</ignoreInComponent>
        </attribute>
        <attribute>
            <name>seriesColors</name>
            <required>false</required>
            <type>java.lang.String</type>
            <ignoreInComponent>true</ignoreInComponent>
        </attribute>
        <attribute>
            <name>bubbleGradients</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
            <defaultValue>false</defaultValue>
        </attribute>
        <attribute>
            <name>bubbleAlpha</name>
            <required>false</required>
            <type>java.lang.Double</type>
            <defaultValue>1.0</defaultValue>
        </attribute>
        <attribute>
            <name>showLabels</name>
            <required>false</required>
            <type>java.lang.Boolean</type>
            <defaultValue>true</defaultValue>
        </attribute>
        <attribute>
            <name>xaxisLabel</name>
            <required>false</required>
            <type>java.lang.String</type>
            <ignoreInComponent>true</ignoreInComponent>
        </attribute>
        <attribute>
            <name>yaxisLabel</name>
            <required>false</required>
            <type>java.lang.String</type>
            <ignoreInComponent>true</ignoreInComponent>
        </attribute>
        <attribute>
            <name>xaxisAngle</name>
            <required>false</required>
            <type>java.lang.Integer</type>
            <ignoreInComponent>true</ignoreInComponent>
        </attribute>
        <attribute>
            <name>yaxisAngle</name>
            <required>false</required>
            <type>java.lang.Integer</type>
            <ignoreInComponent>true</ignoreInComponent>
        </attribute>
        <attribute>
			<name>zoom</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
            <defaultValue>false</defaultValue>
            <description>Enables plot zooming.</description>
		</attribute>
        <attribute>
			<name>extender</name>
			<required>false</required>
			<type>java.lang.Integer</type>
            <description>Javascript function to extend the chart with low level jqplot options.</description>
            <ignoreInComponent>true</ignoreInComponent>
		</attribute>
        <attribute>
			<name>showDatatip</name>
			<required>false</required>
			<type>java.lang.Boolean</type>
            <defaultValue>true</defaultValue>
            <description>Defines visibility of datatip, default is true.</description>
		</attribute>
        <attribute>
			<name>datatipFormat</name>
			<required>false</required>
			<type>java.lang.String</type>
            <description>Template string for datatips.</description>
		</attribute>
    </attributes>
	<resources>
        <resource>
			<name>charts/charts.css</name>
		</resource>
		<resource>
			<name>jquery/jquery.js</name>
		</resource>
		<resource>
			<name>primefaces.js</name>
		</resource>
		<resource>
			<name>charts/charts.js</name>
		</resource>
	</resources>
</component>