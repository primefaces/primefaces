<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE component SYSTEM "../misc/component.dtd" [
<!ENTITY standard_uicomponent_attributes		SYSTEM "../entities/standard_uicomponent_attributes.xml">
]>
<component>
	<tag>toolbarGroup</tag>
	<tagClass>org.primefaces.component.toolbar.ToolbarGroupTag</tagClass>
	<componentClass>org.primefaces.component.toolbar.ToolbarGroup</componentClass>
	<componentType>org.primefaces.component.ToolbarGroup</componentType>
	<componentFamily>org.primefaces.component</componentFamily>
	<parent>javax.faces.component.UIComponentBase</parent>
    <description>ToolbarbarGroup is a helper component for Toolbar component to define placeholders.</description>
	<attributes>
		&standard_uicomponent_attributes;
        <attribute>
			<name>align</name>
			<required>false</required>
			<type>java.lang.String</type>
            <defaultValue>left</defaultValue>
            <description>Defines the alignment within toolbar, valid values are left and right. Default is left.</description>
		</attribute>
        <attribute>
            <name>style</name>
            <required>false</required>
            <type>java.lang.String</type>
            <description>Inline style of the container element.</description>
        </attribute>
        <attribute>
            <name>styleClass</name>
            <required>false</required>
            <type>java.lang.String</type>
            <description>Style class of the container element.</description>
        </attribute>
	</attributes>
</component>
